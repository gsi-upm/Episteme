        <ecos:CompanyName>
          <rdf:Description>
            <ecos:name>ASOCIACIÓN DE EMPRESARIOS DE TECNOLOGÍAS DE LA INFORMACIÓN Y COMUNICACIONES DE ANDALUCÍA</ecos:name>
            <ecos:uri>www.eticom.com</ecos:uri>
